\GetIdInfo$Id: ctex.dtx 07425fe 2021-03-14 16:43:36 +0800 Xiangdong Zeng <xdzeng96@gmail.com> $
{Ubuntu fonts definition (CTEX)}
\ProvidesExplFile{ctex-fontset-dreamclass.def}
{\ExplFileDate}{2.5.6}{\ExplFileDescription}
\ctex_fontset_case:nnnn
{ \ctex_fontset_error:n { dreamclass } }
{
  \ctex_zhmap_case:nnn
  {
    \setCJKmainfont
    [
      BoldFont = SourceHanSerifCN-SemiBold.otf,
      ItalicFont = FZXKTK.TTF,
      BoldItalicFont = FZShengSKSJW_Cu.TTF
    ] { SourceHanSerifCN-Light.otf }
    \setCJKsansfont
    [
      BoldFont = SourceHanSansCN-Medium.otf,
      ItalicFont = FZXKTK.TTF,
      BoldItalicFont = FZShengSKSJW_Cu.TTF
    ] { SourceHanSansCN-Light.otf }
    \setCJKmonofont
    [
      Scale=.9,
      Extension=.ttf,
      BoldFont = sarasa-mono-slab-sc-regular,
      ItalicFont = sarasa-mono-slab-sc-lightitalic,
      BoldItalicFont =sarasa-mono-slab-sc-italic
    ] { sarasa-mono-slab-sc-light }
    \setCJKfamilyfont { zhsong }
    [
      BoldFont = SourceHanSerifCN-SemiBold.otf,
      ItalicFont = FZXKTK.TTF,
      BoldItalicFont = FZShengSKSJW_Cu.TTF
    ] { SourceHanSerifCN-Light.otf }
    \setCJKfamilyfont { zhhei }
    [
      BoldFont = SourceHanSansCN-Medium.otf,
      ItalicFont = FZXKTK.TTF,
      BoldItalicFont = FZShengSKSJW_Cu.TTF
    ] { SourceHanSansCN-Light.otf }
    \setCJKfamilyfont { zhkai }
    [
      BoldFont = FZShengSKSJW_Cu.TTF,
      ItalicFont = SourceHanSerifCN-Light.otf,
      BoldItalicFont = SourceHanSerifCN-SemiBold.otf
    ] { FZXKTK.TTF }
    \setCJKfamilyfont { zhfs }
    [
      Scale=.9,
      Extension=.ttf,
      BoldFont = sarasa-mono-slab-sc-regular,
      ItalicFont = sarasa-mono-slab-sc-lightitalic,
      BoldItalicFont =sarasa-mono-slab-sc-italic
    ] { sarasa-mono-slab-sc-light }
    \ctex_punct_set:n { dreamclass }
    \ctex_punct_map_family:nn   { \CJKrmdefault         } { zhsong  }
    \ctex_punct_map_family:nn   { \CJKsfdefault         } { zhhei   }
    \ctex_punct_map_family:nn   { \CJKttdefault         } { zhfs    }
    \ctex_punct_map_itshape:nn  { \CJKrmdefault         } { zhkai   }
    \ctex_punct_map_bfseries:nn { \CJKsfdefault, zhhei  } { zhheib  }
    \ctex_punct_map_bfseries:nn { \CJKrmdefault, zhsong } { zhsongb }
  }
  {
    \ctex_load_zhmap:nnnn { rm } { zhhei } { zhfs } { dreamclass }
    \ctex_punct_set:n { dreamclass }
    \ctex_punct_map_family:nn   { \CJKrmdefault } { zhsong }
    \ctex_punct_map_family:nn   { \CJKsfdefault } { zhhei  }
    \ctex_punct_map_bfseries:nn { \CJKrmdefault } { zhhei  }
    \ctex_punct_map_itshape:nn  { \CJKrmdefault } { zhkai  }
  }
  { \ctex_fontset_error:n { dreamclass } }
}
{
  \ctex_set_upmap_unicode:nnn { upserif   } { SourceHanSerifCN-Light.otf    } { SourceHanSerifCN-SemiBold.otf   }
  \ctex_set_upmap:nnn         { upserifit } { FZXKTK.TTF                    } { FZShengSKSJW_Cu.TTF             }
  \ctex_set_upmap_unicode:nnn { upsans    } { SourceHanSansCN-Light.otf     } { SourceHanSansCN-Medium.otf      }
  \ctex_set_upmap_unicode:nnn { upmono    } { sarasa-mono-slab-sc-light.ttf } { sarasa-mono-slab-sc-regular.ttf }
  \ctex_set_upfamily:nnn { zhsong } { upzhserif   } { upzhserifb   }
  \ctex_set_upfamily:nnn { zhhei  } { upzhsans    } { upzhsansb    }
  \ctex_set_upfamily:nnn { zhkai  } { upzhserifit } { upzhserifitb }
}
{
  \setCJKmainfont
  [
    BoldFont = Source~Han~Serif~CN~SemiBold,
    ItalicFont = FZNewKai-Z03,
    BoldItalicFont = FZShengShiKaiShuS-B-GB
  ] { Source~Han~Serif~CN~Light }
  \setCJKsansfont
  [
    BoldFont = Source~Han~Sans~CN~Medium,
    ItalicFont = FZNewKai-Z03,
    BoldItalicFont = FZShengShiKaiShuS-B-GB
  ] { Source~Han~Sans~CN~Light }
  \setCJKmonofont
  [
    Scale=.9,
    BoldFont = Sarasa~Mono~Slab~SC,
    ItalicFont = Sarasa~Mono~Slab~SC~Light~Italic,
    BoldItalicFont =Sarasa~Mono~Slab~SC~Italic
  ] { Sarasa~Mono~Slab~SC~Light }
  \setCJKfamilyfont { zhsong }
  [
    BoldFont = Source~Han~Serif~CN~SemiBold,
    ItalicFont = FZNewKai-Z03,
    BoldItalicFont = FZShengShiKaiShuS-B-GB
  ] { Source~Han~Serif~CN~Light }
  \setCJKfamilyfont { zhhei }
  [
    BoldFont = Source~Han~Sans~CN~Medium,
    ItalicFont = FZNewKai-Z03,
    BoldItalicFont = FZShengShiKaiShuS-B-GB
  ] { Source~Han~Sans~CN~Light }
  \setCJKfamilyfont { zhkai }
  [
    BoldFont = FZShengShiKaiShuS-B-GB,
    ItalicFont = Source~Han~Serif~CN~Light,
    BoldItalicFont = Source~Han~Serif~CN~SemiBold
  ] { FZNewKai-Z03 }
  \setCJKfamilyfont { zhfs }
  [
    Scale=.9,
    BoldFont = Sarasa~Mono~Slab~SC,
    ItalicFont = Sarasa~Mono~Slab~SC~Light~Italic,
    BoldItalicFont =Sarasa~Mono~Slab~SC~Italic
  ] { Sarasa~Mono~Slab~SC~Light }
}
\NewDocumentCommand \songti     { } { \CJKfamily { zhsong  } }
\NewDocumentCommand \heiti      { } { \CJKfamily { zhhei   } }
\NewDocumentCommand \kaishu     { } { \CJKfamily { zhkai   } }
\NewDocumentCommand \fangsong   { } { \CJKfamily { zhfs    } }
